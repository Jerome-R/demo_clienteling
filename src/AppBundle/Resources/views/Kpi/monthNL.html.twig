{% extends "AppBundle::layout.html.twig" %}

{% block subheader %}
	{% include 'AppBundle:Commons:kpiNav.html.twig' %}
{% endblock %}

{% block content %}	
	{% block clientsFilter %}
		{% include 'AppBundle:Commons:kpiFilter.html.twig' %}
	{% endblock %}

	{% if kpiCurrentMonth is not null %}

	<div class="col-xs-12">
		<h2 class="text-uppercase gris-bg">
			{{ "Suivi de la capture"|trans }} sur le mois {{ getMonthWording(month, "de") }} {{ kpiCurrentMonth.date|date("Y") }} {#% if kpiCurrentMonth.niveau == 'TOT' %}au Global{% else %}pour {{kpiCurrentMonth.libelle}}{% endif %#}
		</h2>

		<div class="col-xs-12 kpi-box">

			<div class="inside col-xs-12 col-md-4">
				<h3 class="text-uppercase text-center gris">Identification des clients</h3>

				<figure class="col-xs-12">
					<div id="chartContainer0" class="col-xs-12" style="height: 194px;"></div>
				</figure>
				
				<div class="spacer"></div>
				<div class="col-xs-12">
					<p class="col-xs-12 text-center text-uppercase"><br />
						<strong>{{ kpiCurrentMonth.nbTransTotM|number_format(0, ',', ' ') }} Transactions enregistrées, dont {{ kpiCurrentMonth.nbTransLocalM|number_format(0, ',', ' ') }} au niveau local</strong>
					</p>
					<h5 class="gris">Transactions :</h5>
					<p class="small col-xs-12">
						<i class="fa fa-square fa-lg blue" aria-hidden="true"></i> Reliées à une fiche client - locaux<br />
						<i  class="fa fa-square fa-lg orange" aria-hidden="true"></i> Reliées à une fiche client - non locaux<br />				
						<i class="fa fa-square fa-lg gray2" aria-hidden="true"></i> Non reliées à une fiche client
					</p>
				</div>

			</div>

			<div class="inside col-xs-12 col-md-8 bordered-left" >
				<h3 class="text-uppercase text-center gris">&Eacute;volution mensuelle des indicateurs de collecte</h3>

				<figure class="col-xs-12">
					<br>
					<div id="chartContainer5" class="col-xs-12" style="height: 250px;"></div>
				</figure>
				
				<div class="spacer"></div>
				<div class="col-xs-12 text-center">
					<p class="small">
						&nbsp;
					</p>
				</div>
			</div>

			<div class="inside col-xs-12 bordered-top">
				<h3 class="text-uppercase text-center gris">Contactabilité des {{ kpiCurrentMonth.nbCliActifsMNL|number_format(0, ',', ' ') }} Clients actifs<br/>au global et par canal</h3>

				<div class="col-xs-12 col-md-9">
					<figure class="col-xs-3">
						<h4 class="text-center bg-white gris text-uppercase">Au global</h4>
						<div id="chartContainer1" style="height: 150px;"></div>
					</figure>
					<figure class="col-xs-3">
						<h4 class="text-center bg-white gris text-uppercase">Par email</h4>
						<div id="chartContainer2" style="height: 150px;"></div>
					</figure>
					<figure class="col-xs-3">
						<h4 class="text-center bg-white gris text-uppercase">Par téléphone</h4>
						<div id="chartContainer3" style="height: 150px;"></div>
					</figure>
					<figure class="col-xs-3">
						<h4 class="text-center bg-white gris text-uppercase">Par courrier</h4>
						<div id="chartContainer4" style="height: 150px;"></div>
					</figure>
				</div>
				<div class="col-xs-12 col-md-3">
					<figure class="col-xs-12">
						<h4 class="text-center bg-white gris" style="height:55px;">Données non-valides sur le total des données collectées</h4>
						<div id="chartContainer5" style="height: 150px; box-sizing: border-box;padding-top:35px;">
							{% set data_nv = ( (( kpiCurrentMonth.nbCliEmailNonvalidMNL + kpiCurrentMonth.nbCliTelNonvalidMNL + kpiCurrentMonth.nbCliAddNonvalidMNL) / (kpiCurrentMonth.nbCliEmailNonvalidMNL + kpiCurrentMonth.nbCliTelNonvalidMNL + kpiCurrentMonth.nbCliAddNonvalidMNL + 
								kpiCurrentMonth.nbCliEmailValidMNL + kpiCurrentMonth.nbCliTelValidMNL + kpiCurrentMonth.nbCliAddValidMNL)) * 100 )|number_format('0','.','') %}
							<p class="text-center {% if data_nv > 5 %}red{% else %}green{% endif %}" style="font-weight:bold; font-size:42px;color:#aaaaaa">
								{{ data_nv }} %
							</p>
							<p class="small text-center">Part des données email, téléphone et courrier non valides sur l'ensemble des données valides et non valides</p>
						</div>
					</figure>
				</div>

				<div class="col-xs-12 text-center">
					<h4><strong>Objectifs : <span style="color: #f36d35"n>80%</span> de collecte et moins de <span style="color: #f36d35">5%</span> de données non-valides</strong></h4>
				</div>

				<div class="col-xs-12 col-md-4" style="padding:0;margin:0;">				
					<h5 class="gris">Légendes :</h5>
					<p class="small col-xs-12">
						<i class="fa fa-square fa-lg blue" aria-hidden="true"></i> Clients contactables : données valides et optin<br />
						<i class="fa fa-square fa-lg orange" aria-hidden="true"></i> Clients optin mais données non renseignées<br />
						<i  class="fa fa-square fa-lg yellow" aria-hidden="true"></i> Clients optin mais données invalides<br />
						<i class="fa fa-square fa-lg gray2" aria-hidden="true"></i> Clients optout
					</p>
				</div>

				<div class="col-xs-12 col-md-8">
					<h5 class="gris">Clés de lecture :</h5>
					<p class="small col-xs-12">
						Sur les {{ kpiCurrentMonth.nbCliMNL|number_format(0,'',' ') }} clients actifs et identifiés du mois {{ getMonthWording(month, "de") }} {{ kpiCurrentMonth.date|date("Y") }}<br />
						<strong>Au global :</strong>
						{{ kpiCurrentMonth.pctCliCoordValidMNL|number_format(1, ',', ' ') }}% sont contactables (sont optin et ont au moins une donnée de contact renseignée et valide)<br />
						{{ (100.00 - kpiCurrentMonth.pctCliCoordValidMNL)|number_format(1, ',', ' ') }}% sont non contactables dont {{  kpiCurrentMonth.pctOptoutMNL|number_format(1, ',', ' ') }}% de clients optout / {{ kpiCurrentMonth.pctCliCoordNonrensMNL|number_format(1, ',', ' ') }}% optin mais sans donnée renseignée / {{kpiCurrentMonth.pctCliCoordNonvalidMNL|number_format(1, ',', ' ')}}% optin mais sans donnée donnée valide<br />
						<strong>Par canal :</strong>
						{{ kpiCurrentMonth.pctCliEmailValidMNL|number_format(1, ',', ' ') }}% sont contactables email / {{ kpiCurrentMonth.pctCliTelValidMNL|number_format(1, ',', ' ') }}% sont contactables téléphone / {{ kpiCurrentMonth.pctCliAddValidMNL|number_format(1, ',', ' ') }}% sont contactables print
					</p>
				</div>
			</div>
		</div>
	</div>

	<div class="col-xs-12">
		<h2 class="text-uppercase gris-bg">
			Recrutement et fidélisation
		</h2>
		<div class="col-xs-12 kpi-box">

			<div class="inside col-xs-12">
				<h3 class="text-uppercase text-center gris">{{ kpiCurrentMonth.nbCliActifsMNL|number_format(0, ',', ' ') }} Clients actifs<br />sur le mois {{ getMonthWording(month, "de") }} {{ kpiCurrentMonth.date|date("Y") }} dont</h3>
				
				<div class="col-xs-6">
					<h3 class="text-uppercase" style="color: #E8810C;"><i class="fa fa-square fa-lg " aria-hidden="true"></i> 
						{% if  kpiCurrentMonth.nbCliActifsMNL > 0 %}
							{{  kpiCurrentMonth.nbCliActifsNewMNL|number_format("0",",", " ") }}
						{% endif %} clients recrutés en {{ getMonthWording(month, "") }} {{ kpiCurrentMonth.date|date("Y") }}
					</h3>
					{% if kpiLastYear is not null %}
						{% set pct_evol_new = ((( kpiCurrentMonth.nbCliActifsNewMNL - kpiLastYear.nbCliActifsNewMNL) / kpiLastYear.nbCliActifsNewMNL) * 100)|number_format(0, ',', ' ') %}
						<p class="{% if pct_evol_new > 0 %}green{% else %}red{% endif %} big"><i class="fa fa-arrow-{% if pct_evol_new > 0 %}up{% else %}down{% endif %}" aria-hidden="true"></i>
							{% if kpiLastYear.nbCliActifsNewMNL > 0 %}
								{% if  pct_evol_new > 0 %}+{% endif %}{{ pct_evol_new }}%
							{% endif %}
						versus {{ getMonthWording(month, "") }} {{ kpiCurrentMonth.date|date("Y") - 1 }}</p>
					{% endif %}

					<h3 class="text-uppercase" style="color: #2D64AB;"><i class="fa fa-square fa-lg " aria-hidden="true"></i> 
						{% if  kpiCurrentMonth.nbCliActifsMNL > 0 %}
							{{  kpiCurrentMonth.nbCliActifsExistMNL|number_format("0",",", " ") }}
						{% endif %} clients existants en {{ getMonthWording(month, "") }} {{ kpiCurrentMonth.date|date("Y") }}
					</h3>
					{% if kpiLastYear is not null %}
						{% set pct_evol_exist = ((( kpiCurrentMonth.nbCliActifsExistMNL - kpiLastYear.nbCliActifsExistMNL) / kpiLastYear.nbCliActifsExistMNL) * 100)|number_format(0, ',', ' ') %}
						<p class="{% if pct_evol_exist > 0 %}green{% else %}red{% endif %} big"><i class="fa fa-arrow-{% if pct_evol_exist > 0 %}up{% else %}down{% endif %}" aria-hidden="true"></i>
							{% if kpiLastYear.nbCliActifsExistMNL > 0 %}
								{% if  pct_evol_exist > 0 %}+{% endif %}{{ pct_evol_exist }}%
							{% endif %}
							versus {{ getMonthWording(month, "") }} {{ kpiCurrentMonth.date|date("Y") - 1 }}
						</p>
					{% endif %}
				</div>

				<figure class="col-xs-6">
					<div id="chartContainer11" style="height: 300px;"></div>
				</figure>
			</div>
		</div>
	</div>

	<div class="col-xs-12">
		<h2 class="text-uppercase gris-bg">
			{{ "Suivi de la capture des prospects"|trans }}
		</h2>

		<div class="col-xs-12 kpi-box">
			<div class="inside col-xs-12">
				<h3 class="text-uppercase text-center gris">{{ kpiCurrentMonth.nbProspMNL|number_format(0, ',', ' ') }} Prospects enregistrés sur le mois {{ getMonthWording(month, "de") }}  {% if "now"|date("m") == 01 %}{{ "now"|date("Y") - 1 }}{% else %}{{ "now"|date("Y") }}{% endif %}</h3>

				<div class="col-xs-12 col-md-8">
					<figure class="col-xs-3">
						<h4 class="text-center text-uppercase gris">Au global</h4>
						<div id="chartContainer7" style="height: 150px;"></div>
					</figure>
					<figure class="col-xs-3">
						<h4 class="text-center text-uppercase gris">Par email</h4>
						<div id="chartContainer8" style="height: 150px;"></div>
					</figure>
					<figure class="col-xs-3">
						<h4 class="text-center text-uppercase gris">Par téléphone</h4>
						<div id="chartContainer9" style="height: 150px;"></div>
					</figure>

					<figure class="col-xs-3">
						<h4 class="text-center text-uppercase gris">Par courrier</h4>
						<div id="chartContainer10" style="height: 150px;"></div>
					</figure>
				</div>

				
				<div class="col-xs-12  col-md-4">					
					<h5 class="gris">Légende :</h5>
					<p class="small col-xs-12">
						<i class="fa fa-square fa-lg blue" aria-hidden="true"></i> Prospects contactables : données valides et optin<br />
						<i class="fa fa-square fa-lg orange" aria-hidden="true"></i> Prospects optin mais données non renseignées<br />
						<i  class="fa fa-square fa-lg yellow" aria-hidden="true"></i> Prospects optin mais données invalides<br />
						<i class="fa fa-square fa-lg gray2" aria-hidden="true"></i> Prospects optout
					</p>
				</div>
				<div class="col-xs-12">
					<h5 class="gris">Clés de lecture :</h5>
					<p class="small col-xs-12">
						Sur les {{ kpiCurrentMonth.nbProspMNL|number_format(0, ',', ' ') }} prospects identifiés sur le mois {{ getMonthWording(month, "de") }} {% if "now"|date("m") == 01 %}{{ "now"|date("Y") - 1 }}{% else %}{{ "now"|date("Y") }}{% endif %}<br />
						Au global :<br />
						{{ kpiCurrentMonth.pctProspCoordValidMNL|number_format(1, ',', ' ') }}% sont contactables (sont et ont au moins une donnée de contact renseignée et valide)<br />
						{{ (100.00 - kpiCurrentMonth.pctProspCoordValidMNL)|number_format(1, ',', ' ') }}% optin mais sont non contactables dont {{ kpiCurrentMonth.pctProspOptoutMNL|number_format(1, ',', ' ') }}% de clients optout / {{ kpiCurrentMonth.pctProspCoordNonrensMNL|number_format(1, ',', ' ') }}% optin mais sans donnée renseignée / {{ kpiCurrentMonth.pctProspCoordNonvalidMNL|number_format(1, ',', ' ') }}% sans donnée valide<br />
						Par canal :<br />
						{{ kpiCurrentMonth.pctProspEmailValidMNL|number_format(1, ',', ' ') }}% sont contactables email / {{ kpiCurrentMonth.pctProspTelValidMNL|number_format(1, ',', ' ') }}% contactables téléphone / {{ kpiCurrentMonth.pctProspAddValidMNL|number_format(1, ',', ' ') }}% contactable print
					</p>
				</div>
			</div>

			<div class="inside col-xs-12 bordered-top" style="height:313px">
				<h4 class="text-uppercase black text-center gris">&Eacute;volution mensuelle des indicateurs de collecte prospects</h4>
				<br>
				<figure class="col-xs-12 col-md-8 col-md-offset-2">
					<div id="chartContainer6" class="col-xs-12" style="height: 250px;"></div>
				</figure>
				<div class="spacer"></div>
			</div>
		</div>
	</div>
	
	{% else %}
	<div class="col-xs-12">
		<h2 class="text-uppercase gris-bg">
			Aucunes données disponibles pour les données demandées
		</h2>
	</div>

	{% endif %}

	<div style="height:30px;" class="spacer"></div>

{% endblock %}

{% block canvasJs %}

	<script type="text/javascript">

	{% if kpiCurrentMonth is not null %}
	//Init start and end date for charts
	var date = new Date({{ kpiCurrentMonth.date|date("Y") }}, {{ kpiCurrentMonth.date|date("m") - 1 }});
	var month = date.getMonth();
	var year  = date.getFullYear();

	
	var startDate = new Date( year , month - 12)
	var endDate = new Date( year, month +1 );

    CanvasJS.addColorSet("set1",
                [
                "#16a085",
                "#f0f0f0",
                "#aaaaaa",
                ]);
    CanvasJS.addColorSet("set2",
                [
                "#E3009B",
                "#f0f0f0",
                "#aaaaaa",
                ]);
    CanvasJS.addColorSet("set3",
                [
                "#f2ce18",
                "#f0f0f0",
                "#aaaaaa",
                ]);
    CanvasJS.addColorSet("set4",
                [
                "#2D64AB",
                "#f0f0f0",
                "#aaaaaa",
                ]);
    CanvasJS.addColorSet("set5",
                [
                "#16a085",
                "#E3009B",
                "#f2ce18",
                "#2D64AB",              
                ]);

    CanvasJS.addColorSet("set6",
                [
                "#2D64AB",//bleu
                //"#850CE8",//violet
                "#E8810C",//orange
                "#f2ce18",//jaune
                "#aaaaaa",//gris       
                ]);

    CanvasJS.addColorSet("set7",
                [
                "#850CE8",//violet
                "#E8810C",//orange
                "#f2ce18",//jaune
                "#aaaaaa",//gris       
                ]);

    CanvasJS.addColorSet("set8",
                [
                "#2D64AB",//violet
                "#E8810C",//orange
                "#aaaaaa",//gris       
                ]);
    CanvasJS.addCultureInfo("fr",{
        shortMonths: ["JAN", "FÉV", "MAR", "AVR", "MAI", "JUN", "JUI", "AOÛ","SEP", "OCT", "NOV", "DÉC"],
    });

	window.onload = function () {
		var chart = new CanvasJS.Chart("chartContainer1",
		{
			colorSet: "set6",
			animationEnabled: true,
			title:{
				text: {{ (kpiCurrentMonth.pctCliCoordValidMNL)|number_format }} + "%",
				fontSize: 22,
				verticalAlign: "center", // "top", "center", "bottom"
				horizontalAlign: "center", // "left", "right", "center"
				fontColor: "#2c3e50",
			},
			toolTip:{  
			enabled: true
			},
			data: [
			{
				startAngle: -90,  
				indexLabelFontColor: "black",
				indexLabelLineColor: "#4c4c4c",
				indexLabelPlacement: "inside",
				type: "doughnut",
				explodeOnClick: false,
				highlightEnabled: false,
				innerRadius: "75%",
				dataPoints: [
					{  y: {{ (kpiCurrentMonth.pctCliCoordValidMNL) }}, toolTipContent: "{{ (kpiCurrentMonth.pctCliCoordValidMNL)|number_format("0",",", "") }} %<br/>{{ (kpiCurrentMonth.nbCliCoordValidMNL)|number_format(0,'',' ') }} clients"},
					{  y: {{ (kpiCurrentMonth.pctCliCoordNonrensMNL) }}, indexLabel: "", indexLabelFontWeight: "bold", indexLabelFontColor: "white", indexLabelFontSize: 10, toolTipContent: "{{ (kpiCurrentMonth.pctCliCoordNonrensMNL)|number_format("0",",", "") }} %<br/>{{ (kpiCurrentMonth.nbCliCoordNonrensMNL)|number_format(0,'',' ') }} clients"},
					{  y: {{ (kpiCurrentMonth.pctCliCoordNonvalidMNL) }}, indexLabel: "", indexLabelFontWeight: "bold", indexLabelFontColor: "black", indexLabelFontSize: 8, toolTipContent: "{{ (kpiCurrentMonth.pctCliCoordNonvalidMNL)|number_format("0",",", "") }} %<br/>{{ (kpiCurrentMonth.nbCliCoordNonvalidMNL)|number_format(0,'',' ') }} clients"},
					{  y: {{ (kpiCurrentMonth.pctOptoutMNL) }}, indexLabel: "", indexLabelFontWeight: "bold", indexLabelFontColor: "black", indexLabelFontSize: 8, toolTipContent: "{{ (kpiCurrentMonth.pctOptoutMNL)|number_format("0",",", "") }} %<br/>{{ (kpiCurrentMonth.nbOptoutMNL)|number_format(0,'',' ') }} clients"},
				]
			}
			]

		});

		chart.render();
		var chart = new CanvasJS.Chart("chartContainer2",
		{
			colorSet: "set6",
			animationEnabled: true,
			title:{
				text: {{ (kpiCurrentMonth.pctCliEmailValidMNL)|number_format }} + "%",
				fontSize: 22,
				verticalAlign: "center", // "top", "center", "bottom"
				horizontalAlign: "center", // "left", "right", "center"
				fontColor: "#2c3e50",
			},
			toolTip:{  
				enabled: true
			},
			data: [
			{
				startAngle: -90,  
				indexLabelFontColor: "black",
				indexLabelLineColor: "#4c4c4c",

				indexLabelPlacement: "inside",
				type: "doughnut",
				explodeOnClick: false,
				highlightEnabled: false,
				innerRadius: "75%",
				dataPoints: [
					{  y: {{ (kpiCurrentMonth.pctCliEmailValidMNL) }}, indexLabel: "", indexLabelFontWeight: "bold", toolTipContent: "{{ (kpiCurrentMonth.pctCliEmailValidMNL)|number_format("0",",", "") }} %<br/>{{ (kpiCurrentMonth.nbCliEmailValidMNL)|number_format(0,'',' ') }} clients"},
					{  y: {{ (kpiCurrentMonth.pctCliEmailNonrensMNL) }}, indexLabel: "", indexLabelFontSize: 10,indexLabelFontColor: "white",indexLabelFontWeight: "bold",toolTipContent: "{{ (kpiCurrentMonth.pctCliEmailNonrensMNL)|number_format("0",",", "") }} %<br/>{{ (kpiCurrentMonth.nbCliEmailNonrensMNL)|number_format(0,'',' ') }} clients"},
					{  y: {{ (kpiCurrentMonth.pctCliEmailNonvalidMNL) }}, indexLabel: "", indexLabelFontWeight: "bold", indexLabelFontColor: "black", indexLabelFontSize: 10, toolTipContent: "{{ (kpiCurrentMonth.pctCliEmailNonvalidMNL)|number_format("0",",", "") }} %<br/>{{ (kpiCurrentMonth.nbCliEmailNonvalidMNL)|number_format(0,'',' ') }} clients"},
					{  y: {{ (kpiCurrentMonth.pctOptoutMNL) }}, indexLabel: "", indexLabelFontWeight: "bold", indexLabelFontColor: "black", indexLabelFontSize: 8, toolTipContent: "{{ (kpiCurrentMonth.pctOptoutMNL)|number_format("0",",", "") }} %<br/>{{ (kpiCurrentMonth.nbOptoutMNL)|number_format(0,'',' ') }} clients"},
				]
			}
			]

		});

		chart.render();
		var chart = new CanvasJS.Chart("chartContainer3",
		{
			colorSet: "set6",
			animationEnabled: true,
			title:{
				text: {{ (kpiCurrentMonth.pctCliTelValidMNL)|number_format }} + "%",
				fontSize: 22,
				verticalAlign: "center", // "top", "center", "bottom"
				horizontalAlign: "center", // "left", "right", "center"
				fontColor: "#2c3e50",
			},
			toolTip:{  
				enabled: true
			},
			data: [
			{
				startAngle: -90,  
				indexLabelFontColor: "black",
				indexLabelLineColor: "#4c4c4c",

				indexLabelPlacement: "inside",
				type: "doughnut",
				explodeOnClick: false,
				highlightEnabled: false,
				innerRadius: "75%",
				dataPoints: [
					{  y: {{ (kpiCurrentMonth.pctCliTelValidMNL) }}, indexLabel: "",indexLabelFontSize: 10, indexLabelFontWeight: "bold", toolTipContent: "{{ (kpiCurrentMonth.pctCliTelValidMNL)|number_format("0",",", "") }} %<br/>{{ (kpiCurrentMonth.nbCliTelValidMNL)|number_format(0,'',' ') }} clients"},
					{  y: {{ (kpiCurrentMonth.pctCliTelNonrensMNL) }}, indexLabel: "",indexLabelFontSize: 10,indexLabelFontColor: "white", indexLabelFontWeight: "bold",toolTipContent: "{{ (kpiCurrentMonth.pctCliTelNonrensMNL)|number_format("0",",", "") }} %<br/>{{ (kpiCurrentMonth.nbCliTelNonrensMNL)|number_format(0,'',' ') }} clients"},
					{  y: {{ (kpiCurrentMonth.pctCliTelNonvalidMNL) }}, indexLabel: "", indexLabelFontWeight: "bold", indexLabelFontColor: "black", indexLabelFontSize: 10, toolTipContent: "{{ (kpiCurrentMonth.pctCliTelNonvalidMNL)|number_format("0",",", "") }} %<br/>{{ (kpiCurrentMonth.nbCliTelNonvalidMNL)|number_format(0,'',' ') }} clients"},
					{  y: {{ (kpiCurrentMonth.pctOptoutMNL) }}, indexLabel: "", indexLabelFontWeight: "bold", indexLabelFontColor: "black", indexLabelFontSize: 8, toolTipContent: "{{ (kpiCurrentMonth.pctOptoutMNL)|number_format("0",",", "") }} %<br/>{{ (kpiCurrentMonth.nbOptoutMNL)|number_format(0,'',' ') }} clients"},
				]
			}
			]

		});

		chart.render();
		var chart = new CanvasJS.Chart("chartContainer4",
		{
			colorSet: "set6",
			animationEnabled: true,
			title:{
				text: {{ (kpiCurrentMonth.pctCliAddValidMNL)|number_format }} + "%",
				fontSize: 22,
				verticalAlign: "center", // "top", "center", "bottom"
				horizontalAlign: "center", // "left", "right", "center"
				fontColor: "#2c3e50",
			},
			toolTip:{  
				enabled: true
			},
			data: [
			{
				startAngle: -90,  
				indexLabelFontColor: "black",
				indexLabelLineColor: "#4c4c4c",

				indexLabelPlacement: "inside",
				type: "doughnut",
				explodeOnClick: false,
				highlightEnabled: false,
